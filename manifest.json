{

  "manifest_version": 2,
  "name": "speedy-google-music",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://play.google.com/music/listen*"],
      "js": ["speedy-google-music.js"],
      "css": ["speedy-google-music.css"]
    }
  ],

  "icons": { 		"16": "/images/icon16.png", 		"48": "/images/icon48.png", 		"128": "/images/icon128.png" 	}

}